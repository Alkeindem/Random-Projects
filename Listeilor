    client* thirdQueue;
    client* fourthQueue;
    int depositos = 24;
    int giros = 48;
    
    client* head1 = NULL;
    firstQueue = malloc(sizeof(client));
    
    while((depositos > 0) || (giros > 0))
    {
        
    }
    
    firstQueue->task = 0;
    firstQueue->timeOfEspereishon = 0;
    firstQueue->assisted = 1;
    firstQueue->next = malloc(sizeof(client));
    return 0;
}
